<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="syncAndBackups.SyncObjectController">
   <children>
      <Label text="\%source">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="sourceTF" editable="false" prefWidth="400.0" />
      <Label text="\%destination">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="destinationTF" editable="false" prefWidth="400.0" />
      <Label text="\%last_updated">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="lastUpdatedTF" editable="false" prefWidth="400.0" />
      <Label text="\%last_info">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <TextArea fx:id="lastInfoTA" editable="false" prefHeight="200.0" prefWidth="400.0" />
      <HBox alignment="CENTER" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="saveBtn" mnemonicParsing="false" prefWidth="100.0" text="\%save" />
            <Button fx:id="cancelBtn" mnemonicParsing="false" prefWidth="100.0" text="\%cancel" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>

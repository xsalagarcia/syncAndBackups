        <columns>
          <TableColumn fx:id="sourceCol" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="487.0" text="%source" />
          <TableColumn fx:id="lastSyncCol" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="206.0" text="%last_sync" />
        </columns>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </TableView>
      <StackPane maxHeight="1.7976931348623157E308" maxWidth="126.0" minWidth="126.0" prefWidth="126.0">
         <children>
            <Button fx:id="syncBtn" mnemonicParsing="false" text="%synchronize" />
         </children>
      </StackPane>
   </children>
   <padding>
      <Insets left="10.0" />
   </padding>
</HBox>
